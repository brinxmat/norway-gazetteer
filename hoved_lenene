#hovedlenene
#This file defines the adminstrative len structure until the mid 1600s. 
#The start date is the battle of Hafrsfjord (chosen because it was a concrete date for national unification), the end date marks the end of the len system


@prefix : <http://www.ntnu.no/ub/data/gazetteer/len/> .
@prefix foaf: <http://xmlns.com/foaf/0.1/> .
@prefix gn: <http://www.geonames.org/ontology#> .
@prefix ng: <http://www.ntnu.no/ub/data/ng/> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix time: <http://www.w3.org/2006/time#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

:Baahus_len a gn:Feature ;
  a ng:Len ;
  rdfs:label "Baahus len" ;
  foaf:name "Baahus len" ;
  ng:existed [
    a time:Interval ; 
    time:hasBeginning "872"^^xsd:gYear ; 
    time:hasEnd "1662"^^xsd:gYear 
  ] .

:Akershus_len a gn:Feature ;
  a ng:Len ;
  rdfs:label "Akershus len" ;
  foaf:name "Akershus len" ;
  ng:existed [
    a time:Interval ; 
    time:hasBeginning "872"^^xsd:gYear ; 
    time:hasEnd "1662"^^xsd:gYear 
  ] .

:Bergenhus_len a gn:Feature ;
  a ng:Len ;
  rdfs:label "Bergenhus len" ;
  foaf:name "Bergenhus len" ;
  ng:existed [
    a time:Interval ; 
    time:hasBeginning "872"^^xsd:gYear ; 
    time:hasEnd "1662"^^xsd:gYear 
  ] .

:Trondhjems_len a gn:Feature ;
  a ng:Len ;
  rdfs:label "Trondhjems len" ;
  foaf:name "Trondhjems len" ;
  ng:existed [
    a time:Interval ; 
    time:hasBeginning "872"^^xsd:gYear ; 
    time:hasEnd "1662"^^xsd:gYear 
  ] .
  
:Tonsberg_len a gn:Feature ;
  a ng:Len ;
  rdfs:label "Tønsberg len" ;
  foaf:name "Tønsberg len" ;
  ng:existed [
    a time:Interval ;
    time:hasBeginning "1600"^^xsd:gYear ; #imprecise
    time:hasEnd "1673"^^xsd:gYear
  ] ;
  ng:became :grevskap/
